<section class="col-xs-8 col-xs-offset-2 time_entries_day" role="time_entries_day">
  <%= render "tasks/header", tasks: tasks %>

  <div class="row">
    <div class="col-xs-12 well task_list">
      <% tasks.each do |task| %>
        <%= render "tasks/task", task: task %>
      <% end %>
    </div>
  </div>
</section>
